{
  "appEnv": "development",
  "appName": "user-management-service",
  "appPort": "8093",
  "appTimezone": "Asia/Jakarta",
  "secretKey": "user-management-service",

  "dbMysqlDriver": "mysql",
  "dbMysqlHost": "localhost",
  "dbMysqlPort": "3306",
  "dbMysqlDbName": "user_management",
  "dbMysqlUser": "root",
  "dbMysqlPassword": "password",
  "dbMysqlSslMode": "disable",
  "dbMysqlMaxOpenConnections": 10,
  "dbMysqlMaxLifeTimeConnection": 60,
  "dbMysqlMaxIdleConnections": 10,
  "dbMysqlMaxIdleTimeConnection": 30,

  "redisHost": "localhost:6379",

  "kafkaHost": "localhost",
  "kafkaPort": "9092",
  "kafkaTimeoutInMs": 100,
  "kafkaProducerRetryMax": 5,
  "kafkaProducerRetryBackoffInMs": 5
}
